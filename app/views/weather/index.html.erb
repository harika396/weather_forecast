<div class="jumbotron">
  <div class="col-lg-12 mt-5">
    <% flash.each do |type, msg| %>
      <div class="alert alert-info" style="text-align: center;">
        <%= msg %>
      </div>
    <% end %>

    <div class="mx-auto mt-5" style="width: 400px">
      <%= render 'form', autocomplete: @autocomplete %>
    </div>
  </div>
</div>
